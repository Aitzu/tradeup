<!DOCTYPE html>
<html>
  <head>
    <title>TradeUp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= favicon_link_tag asset_path('tradeup_favicon.png'), :rel => 'icon', :type =>  'image/png' %>
  </head>

  <body>
    <%= render "shared/navbar" %>
    <%= link_to "Become a Tradie", edit_user_profile_path(current_user.user_profile.id, :abn_insurance => 0) %>
    <%= yield %>
  </body>
</html>
